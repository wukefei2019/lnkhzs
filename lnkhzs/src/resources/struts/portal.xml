<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">
        
<struts>
	<package name="portalManager" extends="eoms" namespace="/portal">
		<action name="*" method="{1}" class="portalManagerAction">
			<result name="success">/common/portal/{1}.jsp</result>
			<result name="custom">/common/portal/${target}.jsp</result>
		</action>
	</package> 
	<package name="portalAction" extends="eoms" namespace="/portalAction">
		<action name="*" method="{1}" class="portalAction">
			<result name="success">/common/portal/{1}.jsp</result>
			<result name="custom">/common/portal/${target}.jsp</result>
		</action>
	</package> 
</struts>
