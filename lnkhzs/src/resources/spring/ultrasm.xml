	<!-- 用户配置搜索条件 -->
	<bean id="searchConditionDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.eoms.ultrasm.model.SearchCondition</value>
		</property>
	</bean>
	
	
	<bean id="interceptConfigDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
		 	<value>com.ultrapower.eoms.ultrasm.model.InterceptConfig</value>
		</property>
	</bean> 
	
	<bean id="interceptConfigLogDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
		 	<value>com.ultrapower.eoms.ultrasm.model.InterceptConfigLog</value>
		</property>
	</bean> 
	
	
	<bean id="interceptConfigService" class="com.ultrapower.eoms.ultrasm.manager.InterceptConfigImpl" />
	<bean id="interceptConfigAction" class="com.ultrapower.eoms.ultrasm.web.InterceptConfigAction" />
	
</beans>