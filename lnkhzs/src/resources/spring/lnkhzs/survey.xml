	<bean id="khzsSurverManagerService" class="com.ultrapower.lnkhzs.survey.manager.KhzsSurverManagerMgrImpl" />
	
	<bean id="khzsQuestionAnnounceService" class="com.ultrapower.lnkhzs.survey.manager.KhzsQuestionAnnounceMgrImpl" />
	
	<bean id="khzsAnswerAction" class="com.ultrapower.lnkhzs.survey.web.KhzsAnswerAction"/>
	         
	<bean id="khzsQuestionAction" class="com.ultrapower.lnkhzs.survey.web.KhzsQuestionAction"/>
	
	<bean id="khzsQuestionVueAction" class="com.ultrapower.lnkhzs.survey.web.KhzsVueAction"/>
	
	<bean id="khzsQuestionnaireAction" class="com.ultrapower.lnkhzs.survey.web.KhzsQuestionnaireAction"/>	
			
	<bean id="khzsRelationshipAction" class="com.ultrapower.lnkhzs.survey.web.KhzsRelationshipAction"/>
	
	<bean id="dwProductUserInfoAction" class="com.ultrapower.lnkhzs.survey.web.DwProductUserInfoAction"/>	
	
	<bean id="khzsSurverManagerAction" class="com.ultrapower.lnkhzs.survey.web.KhzsSurverManagerAction"/>
		
	<bean id="khzsQuestionAnnounceAction" class="com.ultrapower.lnkhzs.survey.web.KhzsQuestionAnnounceAction"/>
	
	
	
</beans>