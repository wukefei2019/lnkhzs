<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" 
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd" 
	   default-autowire="byName" default-lazy-init="true">

    <bean id="IDaoAOP" class="com.ultrapower.omcs.common.core.dao.IDaoAOP" />

	<!--服务质量管理-->
	<bean id="khzsAnswerDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.KhzsAnswer</value>
		</property>
	</bean>
	
	<bean id="khzsQuestionDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.KhzsQuestion</value>
		</property>
	</bean>
	
	<bean id="khzsQuestionAllDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.KhzsQuestionAll</value>
		</property>
	</bean>
	
	<bean id="khzsQuestionAllHidelistDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.KhzsQuestionAllHidelist</value>
		</property>
	</bean>
	
	<bean id="khzsQuestionnaireDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.KhzsQuestionnaire</value>
		</property>
	</bean>
	
	<bean id="khzsQuestionnaireStyleDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.KhzsQuestionnaireStyle</value>
		</property>
	</bean>
	
	<bean id="khzsRelationshipDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.KhzsRelationship</value>
		</property>
	</bean>
	
	<bean id="bindDicselectDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.BindDicselect</value>
		</property>
	</bean>
	
	 <bean id="bindDicSelectInfoDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.BindDicSelectInfo</value>
		</property>
	</bean> 
	
	<bean id="bindDicselectSymbolDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.BindDicselectSymbol</value>
		</property>
		
	</bean>
	
	<bean id="dwProductUserInfoDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.DwProductUserInfo</value>
		</property>
	</bean>
	
	
	<bean id="khzssurvermanagerDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.KhzsSurverManager</value>
		</property>
	</bean>
	
	<bean id="kzsQuestionAnnounceDao" class="com.ultrapower.eoms.common.core.dao.HibernateDaoImpl">
		<property name="entityClass">
			<value>com.ultrapower.lnkhzs.survey.model.KhzsQuestionAnnounce</value>
		</property>
	</bean>
	
	
   
    
	<bean id="khzsAnswerService" class="com.ultrapower.lnkhzs.survey.manager.KhzsAnswerMgrImpl" />
	
