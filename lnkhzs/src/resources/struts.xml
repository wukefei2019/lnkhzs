<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN" "http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<bean type="com.opensymphony.xwork2.ObjectFactory" name="spring"
		class="com.ultrapower.eoms.common.core.support.StrutsSpringObjectFactory" />


<constant name="struts.enable.DynamicMethodInvocation" value="true" />
    <constant name="struts.strictMethodInvocation.methodRegex"
        value="([A-Za-z0-9_$]*)" />

<package name="eoms" extends="struts-default" strict-method-invocation="false" >
		<interceptors>
			<interceptor-stack name="bpmStack">
				<interceptor-ref name="alias" />
				<interceptor-ref name="servletConfig" />
				<interceptor-ref name="i18n" />
				<interceptor-ref name="chain" />
				
	     		<interceptor-ref name="fileUpload"/>  
        		

				<interceptor-ref name="checkbox" />
				<interceptor-ref name="staticParams" />
				<interceptor-ref name="actionMappingParams" />
				<interceptor-ref name="params">
					<param name="excludeParams">dojo\..*,^struts\..*</param>
				</interceptor-ref>
				<interceptor-ref name="conversionError" />

         		<interceptor-ref name="defaultStack"/> 
			</interceptor-stack>
		</interceptors>
		<default-interceptor-ref name="bpmStack" />
		<global-results>
			<result name="input">/common/core/error.jsp</result>
			<result name="close">/common/core/close.jsp</result>
			<result name="loadxml">/common/core/loadxml.jsp</result>
			<result name="check">/common/core/check.jsp</result>
			<result name="redirect" type="redirect">
				${target}.action
			</result>
			<result name="redirectpar" type="redirect">
				${target}
			</result>
			<result name="closeAndRefreshParent">/omcs/common/closeAndRefreshParent.jsp</result>
            			
		</global-results>
		<global-allowed-methods>regex:.*</global-allowed-methods>
	</package>
	<!-- action的配置文件 -->
	<include file="/struts/common.xml"></include>
	<include file="/struts/portal.xml"/>
	<include file="/struts/ultrasm.xml"/>

	<include file="/struts/ultrasla.xml"/>
    <include file="/struts/lnomcs/baseinfo.xml"/>
	<include file="/struts/lnkhzs/khzs.xml"/>
	<include file="/struts/lnkhzs/quality.xml"/>
	<include file="/struts/lnkhzs/survey.xml"/>
	<include file="/struts/lnkhzs/standards.xml"/>
	<include file="/struts/lnkhzs/tsgd.xml"/>
	<include file="/struts/lnkhzs/trace.xml"/>
	<include file="/struts/lnkhzs/track.xml"/>
	<include file="/struts/lnkhzs/hpdata.xml"/>
	<include file="/struts/lnkhzs/situation.xml"/>
	<include file="/struts/lnkhzs/satisfaction.xml"/>
</struts>

